## =============================================================================================================================================================
## General Purpose I/O
## =============================================================================================================================================================
##
## GPIO SMA
## -----------------------------------------------------------------------------
##	Bank:						13
##		VCCO:					1.8V (VCC1V8_FPGA)
##	Location:				J33, J34
set_property PACKAGE_PIN		AN31				[get_ports VC707_GPIO_SMA_p]
set_property PACKAGE_PIN		AP31				[get_ports VC707_GPIO_SMA_n]
# set I/O standard
set_property IOSTANDARD			LVDS				[get_ports -regexp {VC707_GPIO_SMA_.}]
