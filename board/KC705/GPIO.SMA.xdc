## General Purpose I/O
## =============================================================================================================================================================
##
## GPIO SMA
## -----------------------------------------------------------------------------
##	Bank:						12
##		VCCO:					2.5V (VADJ_FPGA)
##	Location:				J13, J14
set_property PACKAGE_PIN		Y23				[get_ports KC705_GPIO_SMA_p]
set_property PACKAGE_PIN		Y24				[get_ports KC705_GPIO_SMA_n]
# set I/O standard
set_property IOSTANDARD			LVDS_25				[get_ports -regexp {KC705_GPIO_SMA_.}]
